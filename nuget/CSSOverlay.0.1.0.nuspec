<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>CSSOverlay</id>
        <version>0.1.0</version>
        <title>CSS Overlay</title>
        <authors>Jarrett Meyer</authors>
        <projectUrl>http://github.com/jarrettmeyer/cssoverlay</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Provides an overlay for blocking out the main window.</description>
        <language>en-US</language>
        <tags>CSS, JavaScript</tags>
        <dependencies>
            <dependency id="jQuery" version="1.7" />
        </dependencies>
    </metadata>
    <files>
        <file src="..\CSSOverlay\Content\overlay\overlay-spinner.gif" target="content\Content\overlay\overlay-spinner.gif" />
        <file src="..\CSSOverlay\Content\overlay\overlay.css" target="content\Content\overlay\overlay.css" />
        <file src="..\CSSOverlay\Scripts\overlay\overlay.js" target="content\Scripts\overlay\overlay.js" />
        <file src="install.ps1" target="tools\install.ps1" />
        <file src="..\CSSOverlay\Views\Shared\_Overlay.cshtml" target="tools\_Overlay.cshtml" />
    </files>
</package>